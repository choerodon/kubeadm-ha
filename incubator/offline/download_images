#!/bin/sh

docker pull gcr.io/google_containers/kube-proxy:v1.14.3
docker pull gcr.io/google_containers/kube-apiserver:v1.14.3
docker pull gcr.io/google_containers/kube-scheduler:v1.14.3
docker pull gcr.io/google_containers/kube-controller-manager:v1.14.3
docker pull gcr.io/google_containers/coredns:1.3.1
docker pull gcr.io/google_containers/kubernetes-dashboard-amd64:v1.10.1
docker pull gcr.io/google_containers/etcd:3.3.10
docker pull gcr.io/google_containers/metrics-server-amd64:v0.3.3
docker pull quay.io/coreos/flannel:v0.10.0-amd64
docker pull gcr.io/google_containers/pause:3.1
docker pull nginx:1.15.12
docker pull gcr.io/kubernetes-helm/tiller:v2.13.1
docker pull quay.io/kubernetes-ingress-controller/nginx-ingress-controller:0.24.1

docker tag gcr.io/google_containers/kube-proxy:v1.14.3 registry.aliyuncs.com/kubeadm-ha/kube-proxy:v1.14.3
docker tag gcr.io/google_containers/kube-apiserver:v1.14.3 registry.aliyuncs.com/kubeadm-ha/kube-apiserver:v1.14.3
docker tag gcr.io/google_containers/kube-scheduler:v1.14.3 registry.aliyuncs.com/kubeadm-ha/kube-scheduler:v1.14.3
docker tag gcr.io/google_containers/kube-controller-manager:v1.14.3 registry.aliyuncs.com/kubeadm-ha/kube-controller-manager:v1.14.3
docker tag gcr.io/google_containers/coredns:1.3.1 registry.aliyuncs.com/kubeadm-ha/coredns:1.3.1
docker tag gcr.io/google_containers/kubernetes-dashboard-amd64:v1.10.1 registry.aliyuncs.com/kubeadm-ha/kubernetes-dashboard-amd64:v1.10.1
docker tag gcr.io/google_containers/etcd:3.3.10 registry.aliyuncs.com/kubeadm-ha/etcd:3.3.10
docker tag gcr.io/google_containers/metrics-server-amd64:v0.3.3 registry.aliyuncs.com/kubeadm-ha/metrics-server-amd64:v0.3.3
docker tag quay.io/coreos/flannel:v0.10.0-amd64 quay.azk8s.cn/coreos/flannel:v0.11.0-amd64
docker tag gcr.io/google_containers/pause:3.1 registry.aliyuncs.com/kubeadm-ha/pause:3.1
docker tag nginx:1.15.12 dockerhub.azk8s.cn/library/nginx:1.15.12-alpine
docker tag gcr.io/kubernetes-helm/tiller:v2.13.1 gcr.azk8s.cn/kubernetes-helm/tiller:v2.13.1
docker tag quay.io/kubernetes-ingress-controller/nginx-ingress-controller:0.24.1 quay.azk8s.cn/kubernetes-ingress-controller/nginx-ingress-controller:0.24.1

docker save \
    registry.aliyuncs.com/kubeadm-ha/kube-proxy:v1.14.3 \
    registry.aliyuncs.com/kubeadm-ha/kube-apiserver:v1.14.3 \
    registry.aliyuncs.com/kubeadm-ha/kube-scheduler:v1.14.3 \
    registry.aliyuncs.com/kubeadm-ha/kube-controller-manager:v1.14.3 \
    registry.aliyuncs.com/kubeadm-ha/coredns:1.3.1 \
    registry.aliyuncs.com/kubeadm-ha/kubernetes-dashboard-amd64:v1.10.1 \
    registry.aliyuncs.com/kubeadm-ha/etcd:3.3.10 \
    registry.aliyuncs.com/kubeadm-ha/metrics-server-amd64:v0.3.3 \
    registry.aliyuncs.com/kubeadm-ha/pause:3.1 \
    gcr.azk8s.cn/kubernetes-helm/tiller:v2.13.1 \
    dockerhub.azk8s.cn/library/nginx:1.15.12-alpine \
    quay.azk8s.cn/kubernetes-ingress-controller/nginx-ingress-controller:0.24.1 \
    quay.azk8s.cn/coreos/flannel:v0.11.0-amd64 | gzip -1 > kubernetes-1.14.3.tar